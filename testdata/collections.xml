<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../schemas/CiteCollectionInventory.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<collectionService
    xmlns="http://chs.harvard.edu/xmlns/cite"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
     version="0.1">
    <extensions>
        <extension abbr="image" extensionUri="http://chs.harvard.edu/cite/chsimg" rdfType="cite:CiteImage" rdfUri="http://www.homermultitext.org/cite/rdf/citeimage" extendedCitation="true">
            <request name="GetBinaryImage"></request>
            <request name="GetCaption"></request>
            <request name="GetRights"></request>
            <request name="GetImagePlus"></request>
            <request name="GetIIPMooViewer"></request>
        </extension>
    </extensions>
   
    
    <!-- IMAGE DATA FROM HMT Collections ********************   -->
    <citeCollection canonicalId="Image" label="Caption" urn="urn:cite:hmt:vaimg">
        <namespaceMapping
            abbr="hmt"
            uri="http://www.homermultitext.org/datans"/>
        
        <extendedBy abbr="image"/>
        <dc:title
            xmlns="http://purl.org/dc/elements/1.1/">Images of the Venetus A MS</dc:title>
        <creator
            xmlns="http://purl.org/dc/elements/1.1/">Christopher Blackwell</creator>
        <creator
            xmlns="http://purl.org/dc/elements/1.1/">Neel Smith</creator>
        <coverage
            xmlns="http://purl.org/dc/elements/1.1/">All images from the Homer Multitext project's photography of the Venetus A manuscript.</coverage>
        <description
            xmlns="http://purl.org/dc/elements/1.1/"> Photography of the Venetus A manuscript by the Homer Multitext project.</description>
        <rights
            xmlns="http://purl.org/dc/elements/1.1/"> All data in this collection are available
            under the terms of the Creative Commons Attribution-Non-Commercial 3.0 Unported License,
            http://creativecommons.org/licenses/by-nc/3.0/deed.en_US</rights>
        <source
            type="file"
            value="vaimgs.csv"/>
        
        <citeProperty name="Image" label="Image URN" type="citeurn"/>
        <citeProperty name="Label" label="Caption" type="string"/>
        <citeProperty name="Rights" label="Rights" type="string">
        </citeProperty>
        
    </citeCollection>
    
    <citeCollection
        canonicalId="URN"
        label="EnglishName"
        urn="urn:cite:hmt:critsigns">
        <namespaceMapping
            abbr="hmt"
            uri="http://www.homermultitext.org/datans"/>
        <dc:title>Classes of Aristarchan critical signs</dc:title>
        <dc:description>Names and character encoding information for Aristarchan critical
            signs</dc:description>
        <dc:rights>All data in this collection are available under the terms of the Creative Commons
            Attribution-Non-Commercial 3.0 Unported License,
            http://creativecommons.org/licenses/by-nc/3.0/deed.en_US</dc:rights>
        <source
            type="file"
            value="criticalsigns.tsv"/>
        <citeProperty
            label="URN"
            name="URN"
            type="citeurn"/>
        <citeProperty
            label="English name"
            name="EnglishName"
            type="string"/>
        <citeProperty
            label="Greek name"
            name="GreekName"
            type="string"/>
        <citeProperty
            label="Unicode codepoint (hex value)"
            name="UnicodeHex"
            type="string"/>
        <citeProperty
            label="Notes on Unicode representation"
            name="UnicodeNotes"
            type="string"/>
    </citeCollection>
    
    <citeCollection
        canonicalId="URN"
        label="Label"
        urn="urn:cite:hmt:msA">
        <namespaceMapping
            abbr="hmt"
            uri="http://www.homermultitext.org/datans"/>
        <title
            xmlns="http://purl.org/dc/elements/1.1/">Pages of the Venetus A manuscript</title>
        <creator
            xmlns="http://purl.org/dc/elements/1.1/">Christopher Blackwell</creator>
        <creator
            xmlns="http://purl.org/dc/elements/1.1/">Neel Smith</creator>
        <coverage
            xmlns="http://purl.org/dc/elements/1.1/">All folio sides of the Venetus A
            manuscript</coverage>
        <description
            xmlns="http://purl.org/dc/elements/1.1/">Folio sides of the Venetus A
            manuscript.</description>
        <rights
            xmlns="http://purl.org/dc/elements/1.1/"> All data in this collection are available
            under the terms of the Creative Commons Attribution-Non-Commercial 3.0 Unported License,
            http://creativecommons.org/licenses/by-nc/3.0/deed.en_US</rights>
        <orderedBy
            property="Sequence"/>
        <!-- // Siglum	Sequence	URN	RV	Label	CodexURN -->
        
        <source
            type="file"
            value="venetusA.tsv"/>
        <citeProperty
            label="URN"
            name="URN"
            type="citeurn"/>
        <citeProperty
            label="Siglum"
            name="Siglum"
            type="string"/>
        <citeProperty
            label="Sequence"
            name="Sequence"
            type="number"/>
       
       
        <citeProperty
            label="Recto or verso"
            name="RV"
            type="string"/>
        <citeProperty
            label="description"
            name="Label"
            type="string"/>
        <citeProperty
            label="Codex URN"
            name="CodexURN"
            type="citeurn"/>
    </citeCollection>
  
    
</collectionService>
